#include <stdio.h>

int main() {
    int num1, num2, num3, num4;
    int troca;

    printf("Digite 5 números:\n");
    
    printf("Número 1: ");
    
    scanf("%d", &num1);
    
    printf("Número 2: ");
    
    scanf("%d", &num2);
    
    printf("Número 3: ");
    
    scanf("%d", &num3);
    
    printf("Número 4: ");
    
    scanf("%d", &num4);


    if (num1 > num2) { 
	troca = num1; num1 = num2; num2 = troca; 
    
    
    }
	
    if (num1 > num3) {
	 troca = num1; num1 = num3; num3 = troca; 
    }
	 
   
    if (num1 > num4) { 
	troca = num1; num1 = num4; num4 = troca; 
    
        
    }
	
	 
    if (num2 > num3) { 
	troca = num2; num2 = num3; num3 = troca; 
    }
	
    if (num2 > num4) {
	 troca = num2; num2 = num4; num4 = troca; 
    }
	 
	 
    if (num3 > num4) {
	 troca = num3; num3 = num4; num4 = troca; 
    }
	 
	printf("Os n em ordem crescente:\n");
    printf("%d %d %d %d\n", num1, num2, num3, num4);

    return 0;
}




